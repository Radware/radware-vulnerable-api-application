{
  "name": "PaulaGreen - Legacy + B2B Discovery",
  "description": "Protected user PaulaGreen logs in, uses legacy address search and legacy order status, then performs a B2B partner lookup.",
  "headers": {
    "Accept": "application/json",
    "x-remote-ip": "{{RANDOM_IP}}"
  },
  "staticVars": {
    "baseUrl": "http://localhost:8060",
    "userUsername": "PaulaGreen",
    "userPassword": "PaulaPass15!",
    "userId": "00000016-0000-0000-0000-000000000016",
    "legacyOrderId": "ab916c29-95de-5be8-b762-be4c584bd706",
    "legacyToken": "legacy-demo",
    "addressSearchQuery": "Coastland",
    "partnerCode": "paula125een"
  },
  "steps": [
    {
      "id": "step_paulagreen_login",
      "name": "Login PaulaGreen",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{userUsername}}&password={{userPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_paulagreen_legacy_address_search",
      "name": "PaulaGreen: Legacy Address Search",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/addresses/search-legacy?q={{addressSearchQuery}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_paulagreen_legacy_order_status",
      "name": "PaulaGreen: Legacy Order Status",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/orders/{{legacyOrderId}}/status-legacy?legacy_token={{legacyToken}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_paulagreen_b2b_partner_lookup",
      "name": "PaulaGreen: B2B Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_paulagreen_login": {
      "x": 50,
      "y": 50
    },
    "step_paulagreen_legacy_address_search": {
      "x": 250,
      "y": 50
    },
    "step_paulagreen_legacy_order_status": {
      "x": 450,
      "y": 50
    },
    "step_paulagreen_b2b_partner_lookup": {
      "x": 650,
      "y": 50
    }
  }
}
