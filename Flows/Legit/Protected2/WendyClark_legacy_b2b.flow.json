{
  "name": "WendyClark - Legacy + B2B Discovery",
  "description": "Protected user WendyClark logs in, uses legacy address search and legacy order status, then performs a B2B partner lookup.",
  "headers": {
    "Accept": "application/json",
    "x-remote-ip": "{{RANDOM_IP}}"
  },
  "staticVars": {
    "baseUrl": "https://bla-bypass.ase-team.com",
    "userUsername": "WendyClark",
    "userPassword": "WendyPass22!",
    "userId": "00000023-0000-0000-0000-000000000023",
    "legacyOrderId": "c585cdb3-8b68-5366-8000-af2baa4ddc47",
    "legacyToken": "legacy-demo",
    "addressSearchQuery": "Exampleburg",
    "partnerCode": "wendy129ark"
  },
  "steps": [
    {
      "id": "step_wendyclark_login",
      "name": "Login WendyClark",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{userUsername}}&password={{userPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_wendyclark_legacy_address_search",
      "name": "WendyClark: Legacy Address Search",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/addresses/search-legacy?q={{addressSearchQuery}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_wendyclark_legacy_order_status",
      "name": "WendyClark: Legacy Order Status",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/orders/{{legacyOrderId}}/status-legacy?legacy_token={{legacyToken}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_wendyclark_b2b_partner_lookup",
      "name": "WendyClark: B2B Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_wendyclark_login": {
      "x": 50,
      "y": 50
    },
    "step_wendyclark_legacy_address_search": {
      "x": 250,
      "y": 50
    },
    "step_wendyclark_legacy_order_status": {
      "x": 450,
      "y": 50
    },
    "step_wendyclark_b2b_partner_lookup": {
      "x": 650,
      "y": 50
    }
  }
}
