{
  "name": "admin - Legacy + B2B Discovery",
  "description": "Protected user admin logs in, uses legacy address search and legacy order status, then performs a B2B partner lookup.",
  "headers": {
    "Accept": "application/json",
    "x-remote-ip": "{{RANDOM_IP}}"
  },
  "staticVars": {
    "baseUrl": "https://bla-bypass.ase-team.com",
    "userUsername": "admin",
    "userPassword": "AdminPass123!",
    "userId": "00000001-0000-0000-0000-000000000001",
    "legacyOrderId": "4e6c5038-d0f6-5f76-85ba-6f236b4cd163",
    "legacyToken": "legacy-demo",
    "addressSearchQuery": "AdminCity",
    "partnerCode": "admin111min"
  },
  "steps": [
    {
      "id": "step_admin_login",
      "name": "Login admin",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{userUsername}}&password={{userPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_admin_legacy_address_search",
      "name": "admin: Legacy Address Search",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/addresses/search-legacy?q={{addressSearchQuery}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_admin_legacy_order_status",
      "name": "admin: Legacy Order Status",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/orders/{{legacyOrderId}}/status-legacy?legacy_token={{legacyToken}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_admin_b2b_partner_lookup",
      "name": "admin: B2B Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_admin_login": {
      "x": 50,
      "y": 50
    },
    "step_admin_legacy_address_search": {
      "x": 250,
      "y": 50
    },
    "step_admin_legacy_order_status": {
      "x": 450,
      "y": 50
    },
    "step_admin_b2b_partner_lookup": {
      "x": 650,
      "y": 50
    }
  }
}
