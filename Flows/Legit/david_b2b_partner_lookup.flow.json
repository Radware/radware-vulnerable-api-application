{
  "name": "DavidBrown - B2B Partner Lookup (Short)",
  "description": "Protected user DavidBrown logs in and uses the legacy B2B partner lookup endpoint in a read-only way.",
  "headers": {
    "Accept": "application/json"
  },
  "staticVars": {
    "baseUrl": "https://bla-secure.ase-team.com",
    "davidUsername": "DavidBrown",
    "davidPassword": "DavidPass4$",
    "partnerCode": "cjdjd654dkd"
  },
  "steps": [
    {
      "id": "step_david_login_b2b",
      "name": "Login DavidBrown",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{davidUsername}}&password={{davidPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_david_b2b_lookup",
      "name": "B2B: Legacy Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_david_login_b2b": { "x": 50, "y": 50 },
    "step_david_b2b_lookup": { "x": 250, "y": 50 }
  }
}
