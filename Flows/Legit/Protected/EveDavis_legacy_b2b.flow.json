{
  "name": "EveDavis - Legacy + B2B Discovery",
  "description": "Protected user EveDavis logs in, uses legacy address search and legacy order status, then performs a B2B partner lookup.",
  "headers": {
    "Accept": "application/json"
  },
  "staticVars": {
    "baseUrl": "http://localhost:8060",
    "userUsername": "EveDavis",
    "userPassword": "EvePass5%",
    "userId": "00000006-0000-0000-0000-000000000006",
    "legacyOrderId": "671c7092-a373-5b56-931d-253fde055a2b",
    "legacyToken": "legacy-demo",
    "addressSearchQuery": "Smallville",
    "partnerCode": "eveda116vis"
  },
  "steps": [
    {
      "id": "step_evedavis_login",
      "name": "Login EveDavis",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{userUsername}}&password={{userPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_evedavis_legacy_address_search",
      "name": "EveDavis: Legacy Address Search",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/addresses/search-legacy?q={{addressSearchQuery}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_evedavis_legacy_order_status",
      "name": "EveDavis: Legacy Order Status",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/orders/{{legacyOrderId}}/status-legacy?legacy_token={{legacyToken}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_evedavis_b2b_partner_lookup",
      "name": "EveDavis: B2B Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_evedavis_login": {
      "x": 50,
      "y": 50
    },
    "step_evedavis_legacy_address_search": {
      "x": 250,
      "y": 50
    },
    "step_evedavis_legacy_order_status": {
      "x": 450,
      "y": 50
    },
    "step_evedavis_b2b_partner_lookup": {
      "x": 650,
      "y": 50
    }
  }
}
