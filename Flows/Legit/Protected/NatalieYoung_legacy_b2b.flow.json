{
  "name": "NatalieYoung - Legacy + B2B Discovery",
  "description": "Protected user NatalieYoung logs in, uses legacy address search and legacy order status, then performs a B2B partner lookup.",
  "headers": {
    "Accept": "application/json"
  },
  "staticVars": {
    "baseUrl": "https://bla-bypass.ase-team.com",
    "userUsername": "NatalieYoung",
    "userPassword": "NataliePass13!",
    "userId": "00000014-0000-0000-0000-000000000014",
    "legacyOrderId": "5d9b6870-72fa-5abd-9f68-91926339f1ba",
    "legacyToken": "legacy-demo",
    "addressSearchQuery": "Riverdale",
    "partnerCode": "natal123ung"
  },
  "steps": [
    {
      "id": "step_natalieyoung_login",
      "name": "Login NatalieYoung",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{userUsername}}&password={{userPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_natalieyoung_legacy_address_search",
      "name": "NatalieYoung: Legacy Address Search",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/addresses/search-legacy?q={{addressSearchQuery}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_natalieyoung_legacy_order_status",
      "name": "NatalieYoung: Legacy Order Status",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/orders/{{legacyOrderId}}/status-legacy?legacy_token={{legacyToken}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_natalieyoung_b2b_partner_lookup",
      "name": "NatalieYoung: B2B Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_natalieyoung_login": {
      "x": 50,
      "y": 50
    },
    "step_natalieyoung_legacy_address_search": {
      "x": 250,
      "y": 50
    },
    "step_natalieyoung_legacy_order_status": {
      "x": 450,
      "y": 50
    },
    "step_natalieyoung_b2b_partner_lookup": {
      "x": 650,
      "y": 50
    }
  }
}
