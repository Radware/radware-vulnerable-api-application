{
  "name": "LauraScott - Legacy + B2B Discovery",
  "description": "Protected user LauraScott logs in, uses legacy address search and legacy order status, then performs a B2B partner lookup.",
  "headers": {
    "Accept": "application/json"
  },
  "staticVars": {
    "baseUrl": "http://localhost:8060",
    "userUsername": "LauraScott",
    "userPassword": "LauraPass11!",
    "userId": "00000012-0000-0000-0000-000000000012",
    "legacyOrderId": "652d82c2-e3f1-5183-b586-d2f82f48a716",
    "legacyToken": "legacy-demo",
    "addressSearchQuery": "Emerald%20City",
    "partnerCode": "laura121ott"
  },
  "steps": [
    {
      "id": "step_laurascott_login",
      "name": "Login LauraScott",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{userUsername}}&password={{userPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_laurascott_legacy_address_search",
      "name": "LauraScott: Legacy Address Search",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/addresses/search-legacy?q={{addressSearchQuery}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_laurascott_legacy_order_status",
      "name": "LauraScott: Legacy Order Status",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/orders/{{legacyOrderId}}/status-legacy?legacy_token={{legacyToken}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_laurascott_b2b_partner_lookup",
      "name": "LauraScott: B2B Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_laurascott_login": {
      "x": 50,
      "y": 50
    },
    "step_laurascott_legacy_address_search": {
      "x": 250,
      "y": 50
    },
    "step_laurascott_legacy_order_status": {
      "x": 450,
      "y": 50
    },
    "step_laurascott_b2b_partner_lookup": {
      "x": 650,
      "y": 50
    }
  }
}
