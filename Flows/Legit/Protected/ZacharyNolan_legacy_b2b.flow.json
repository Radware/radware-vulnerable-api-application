{
  "name": "ZacharyNolan - Legacy + B2B Discovery",
  "description": "Protected user ZacharyNolan logs in, uses legacy address search and legacy order status, then performs a B2B partner lookup.",
  "headers": {
    "Accept": "application/json"
  },
  "staticVars": {
    "baseUrl": "https://bla-bypass.ase-team.com",
    "userUsername": "ZacharyNolan",
    "userPassword": "ZacharyPass25!",
    "userId": "00000026-0000-0000-0000-000000000026",
    "legacyOrderId": "68b48e04-9ebc-5603-87e9-56028c8de20e",
    "legacyToken": "legacy-demo",
    "addressSearchQuery": "Demo%20Town",
    "partnerCode": "zacha133lan"
  },
  "steps": [
    {
      "id": "step_zacharynolan_login",
      "name": "Login ZacharyNolan",
      "type": "request",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login?username={{userUsername}}&password={{userPassword}}",
      "extract": {
        "token": "body.access_token"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_zacharynolan_legacy_address_search",
      "name": "ZacharyNolan: Legacy Address Search",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/addresses/search-legacy?q={{addressSearchQuery}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_zacharynolan_legacy_order_status",
      "name": "ZacharyNolan: Legacy Order Status",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/users/{{userId}}/orders/{{legacyOrderId}}/status-legacy?legacy_token={{legacyToken}}",
      "headers": {
        "Authorization": "Bearer {{token}}"
      },
      "onFailure": "stop"
    },
    {
      "id": "step_zacharynolan_b2b_partner_lookup",
      "name": "ZacharyNolan: B2B Partner Lookup",
      "type": "request",
      "method": "GET",
      "url": "{{baseUrl}}/api/b2b/partner-lookup?q={{partnerCode}}",
      "onFailure": "continue"
    }
  ],
  "visualLayout": {
    "step_zacharynolan_login": {
      "x": 50,
      "y": 50
    },
    "step_zacharynolan_legacy_address_search": {
      "x": 250,
      "y": 50
    },
    "step_zacharynolan_legacy_order_status": {
      "x": 450,
      "y": 50
    },
    "step_zacharynolan_b2b_partner_lookup": {
      "x": 650,
      "y": 50
    }
  }
}
